body = new div in window
body apply new style { 
  width: 100%
  height: 100vh
  bg: gray
  alignment: col
}

card = new div in body
card apply new style {
  width: 90%
  maxWidth: 400px
  bg: white
  height: 300px
  alignment: row wrap reverse
}

cardItemStl = new style {
  width: 30px
  height: 30px
  bg: #0078aa
  margin: 10px
}

a = new 4 div in card apply cardItemStl
b = new 3 div in card apply new style {
  width: 30px
  height: 30px
  borderRadius: 50%
  bg: green
  margin: 35px
}